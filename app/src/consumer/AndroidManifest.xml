
<manifest xmlns:android="http://schemas.android.com/apk/res/android">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />

    <application>
        <activity android:name="e2b.activity.AuthActivity" android:screenOrientation="portrait"></activity>
        <activity android:name="e2b.activity.HelpActivity" android:screenOrientation="portrait"></activity>
        <activity android:name="e2b.activity.MapActivity" android:screenOrientation="portrait"></activity>
        <activity android:name="e2b.activity.ProfileActivity" android:screenOrientation="portrait"></activity>
        <activity android:name="e2b.activity.OrdersActivity" android:screenOrientation="portrait"></activity>
        <activity android:name="e2b.activity.MerchantDetailActivity" android:screenOrientation="portrait"></activity>
        <activity android:name="e2b.activity.NotificationActivity" android:screenOrientation="portrait"></activity>
        <activity android:name="e2b.activity.OrderDetailActivity" android:screenOrientation="portrait"></activity>
        <activity android:name="e2b.activity.PlaceOrderActivity" android:screenOrientation="portrait"></activity>
        <activity android:name="e2b.activity.AudioRecordingActivity" android:screenOrientation="portrait"></activity>
        <activity android:name="e2b.activity.AudioPlaybackActivity" android:screenOrientation="portrait"></activity>

        <service android:name="e2b.fcm.FCMMessagingService">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>
        <service android:name="e2b.fcm.FCMTokenUpdateService">
            <intent-filter>
                <action android:name="com.google.firebase.INSTANCE_ID_EVENT" />
            </intent-filter>
        </service>

    </application>




</manifest>